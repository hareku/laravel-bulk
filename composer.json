{
  "name": "hareku/laravel-bulk",
  "type": "library",
  "license": "MIT",
  "description": "The Laravel package for bulk insert/update.",
  "authors": [
    {
      "name": "hareku",
      "email": "hareku908@gmail.com"
    }
